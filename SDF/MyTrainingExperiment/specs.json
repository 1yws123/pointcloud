{
    "Description": "Training with the original script on my 50 heart models.",
    "DataSource": "F:/Code/pointnet2/DeepSDF/Data2",
    "TrainSplit": "F:/Code/pointnet2/DeepSDF/train_split.json",

    "NetworkArch": "deep_sdf_decoder",
    "NetworkSpecs": {
        "dims": [ 512, 512, 512, 512, 512, 512, 512, 512 ],
        "dropout": [0, 1, 2, 3, 4, 5, 6, 7],
        "dropout_prob": 0.2,
        "norm_layers": [0, 1, 2, 3, 4, 5, 6, 7],
        "latent_in": [4],
        "weight_norm": true
    },
    "CodeLength": 256,

    "NumEpochs": 2001,
    "SnapshotFrequency": 500,
    "AdditionalSnapshots": [],

    "LearningRateSchedule": [
        {
            "Type": "Step",
            "Initial": 0.0001,
            "Interval": 500,
            "Factor": 0.5
        },
        {
            "Type": "Step",
            "Initial": 0.001,
            "Interval": 500,
            "Factor": 0.5
        }
    ],

    "SamplesPerScene": 4096,
    "ScenesPerBatch": 4,
    "DataLoaderThreads": 4,

    "ClampingDistance": 0.1,
    "CodeRegularization": true,
    "CodeRegularizationLambda": 1e-4,
    "CodeBound": 1.0
}
